import 'package:flutter/material.dart';
import 'package:http/http.dart' as http;
import 'dart:convert';
import 'models/quote.dart';

void main() {
  runApp(const QuotesApp());
}

// StatelessWidget : MyApp
// StatelessWidget (state)  .
// ,        .
//   (,  )   StatelessWidget .
class QuotesApp extends StatelessWidget {
  const QuotesApp({super.key});

  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      title: ' ',
      theme: ThemeData(
        colorScheme: ColorScheme.fromSeed(seedColor: Colors.brown),
        useMaterial3: true,
      ),
      // StatefulWidget : QuotePage   
      home: const QuotePage(),
    );
  }
}

// StatelessWidget : QuoteCard
//        .
//     StatelessWidget .
//
// StatelessWidget :
// - build()   
// - setState()   
// -     
class QuoteCard extends StatelessWidget {
  final Quote quote;

  const QuoteCard({super.key, required this.quote});

  @override
  Widget build(BuildContext context) {
    return Card(
      margin: const EdgeInsets.all(16.0),
      elevation: 4,
      child: Padding(
        padding: const EdgeInsets.all(20.0),
        child: Column(
          crossAxisAlignment: CrossAxisAlignment.start,
          mainAxisSize: MainAxisSize.min,
          children: [
            //  
            Text(
              quote.message,
              style: const TextStyle(
                fontSize: 18,
                height: 1.6,
                fontWeight: FontWeight.w400,
              ),
            ),
            const SizedBox(height: 24),
            // 
            const Divider(),
            const SizedBox(height: 16),
            //  
            Text(
              quote.author,
              style: const TextStyle(fontSize: 16, fontWeight: FontWeight.bold),
            ),
            const SizedBox(height: 4),
            //  
            Text(
              quote.authorProfile,
              style: TextStyle(fontSize: 14, color: Colors.grey[600]),
            ),
          ],
        ),
      ),
    );
  }
}

// StatefulWidget : QuotePage
//   (state) .
// API  ,   ,     .
//
// StatefulWidget :
// - State    (_QuotePageState)
// - setState()    UI   
// -   build()     
class QuotePage extends StatefulWidget {
  const QuotePage({super.key});

  @override
  State<QuotePage> createState() => _QuotePageState();
}

// State :      
class _QuotePageState extends State<QuotePage> {
  //  
  Quote? _quote; //    (null  )
  bool _isLoading = false; //   
  String? _errorMessage; //   ()

  //      
  @override
  void initState() {
    super.initState();
    //        
    _fetchQuote();
  }

  // API   
  // async/await    
  Future<void> _fetchQuote() async {
    //   
    setState(() {
      _isLoading = true;
      _errorMessage = null;
    });

    try {
      // HTTP GET  
      final response = await http.get(
        Uri.parse('https://korean-advice-open-api.vercel.app/api/advice'),
      );

      // HTTP    (200  )
      if (response.statusCode == 200) {
        // JSON  Dart (Map) 
        final jsonData = json.decode(response.body) as Map<String, dynamic>;

        // Quote  
        final quote = Quote.fromJson(jsonData);

        // setState()   
        //   build()    UI 
        setState(() {
          _quote = quote;
          _isLoading = false;
        });
      } else {
        // HTTP   
        setState(() {
          _errorMessage = '  . (${response.statusCode})';
          _isLoading = false;
        });
      }
    } catch (e) {
      //      
      setState(() {
        _errorMessage = ' : $e';
        _isLoading = false;
      });
    }
  }

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: AppBar(
        backgroundColor: Theme.of(context).colorScheme.inversePrimary,
        title: const Text(' '),
      ),
      body: Center(child: _buildBody()),
      //    
      floatingActionButton: FloatingActionButton.extended(
        onPressed: _isLoading ? null : _fetchQuote,
        icon: _isLoading
            ? const SizedBox(
                width: 20,
                height: 20,
                child: CircularProgressIndicator(
                  strokeWidth: 2,
                  valueColor: AlwaysStoppedAnimation<Color>(Colors.white),
                ),
              )
            : const Icon(Icons.refresh),
        label: const Text(''),
      ),
      floatingActionButtonLocation: FloatingActionButtonLocation.centerFloat,
    );
  }

  // body   
  Widget _buildBody() {
    //   
    if (_isLoading && _quote == null) {
      return const Column(
        mainAxisAlignment: MainAxisAlignment.center,
        children: [
          CircularProgressIndicator(),
          SizedBox(height: 16),
          Text('  ...'),
        ],
      );
    }

    //   
    if (_errorMessage != null && _quote == null) {
      return Padding(
        padding: const EdgeInsets.all(16.0),
        child: Column(
          mainAxisAlignment: MainAxisAlignment.center,
          children: [
            const Icon(Icons.error_outline, size: 64, color: Colors.red),
            const SizedBox(height: 16),
            Text(
              _errorMessage!,
              textAlign: TextAlign.center,
              style: const TextStyle(fontSize: 16),
            ),
            const SizedBox(height: 16),
            ElevatedButton(onPressed: _fetchQuote, child: const Text(' ')),
          ],
        ),
      );
    }

    //   
    if (_quote != null) {
      return SingleChildScrollView(child: QuoteCard(quote: _quote!));
    }

    //   (     )
    return const Text('   .');
  }
}
</file>

