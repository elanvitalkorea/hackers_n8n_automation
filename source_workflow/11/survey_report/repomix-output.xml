CREATE OR REPLACE FUNCTION calculate_current_nps()
RETURNS float
LANGUAGE sql
AS $$
  SELECT
    COALESCE(
      (
        (
          COUNT(CASE WHEN satisfaction >= 9 THEN 1 END) -- Promoters
          - 
          COUNT(CASE WHEN satisfaction <= 6 THEN 1 END) -- Detractors
        )::float / COUNT(id)::float
      ) * 100
    , 0) -- ì‘ë‹µìê°€ 0ëª…ì¼ ê²½ìš° 0 ë°˜í™˜
  FROM surveys;
$$;


-- ----------------------------------------------------------------
-- í•¨ìˆ˜ 2: get_dashboard_stats()
-- ì—­í• : ëŒ€ì‹œë³´ë“œì— í•„ìš”í•œ ëª¨ë“  í†µê³„ ë°ì´í„°ë¥¼ JSON ê°ì²´ë¡œ í•œ ë²ˆì— ë°˜í™˜í•©ë‹ˆë‹¤. (/api/stats ìš©)
-- ----------------------------------------------------------------
CREATE OR REPLACE FUNCTION get_dashboard_stats()
RETURNS jsonb
LANGUAGE sql
AS $$
  SELECT jsonb_build_object(
    'total_respondents',
    (SELECT COUNT(id) FROM surveys),
    
    'nps_score',
    (SELECT calculate_current_nps()), -- ìœ„ì—ì„œ ë§Œë“  í•¨ìˆ˜ ì¬ì‚¬ìš©
    
    'position_distribution',
    (
      SELECT COALESCE(jsonb_agg(stats), '[]'::jsonb)
      FROM (
        SELECT position, COUNT(id) AS count
        FROM surveys
        GROUP BY position
        ORDER BY count DESC
      ) AS stats
    ),
    
    'company_size_distribution',
    (
      SELECT COALESCE(jsonb_agg(stats), '[]'::jsonb)
      FROM (
        SELECT company_size, COUNT(id) AS count
        FROM surveys
        GROUP BY company_size
        ORDER BY count DESC
      ) AS stats
    )
  );
$$;
